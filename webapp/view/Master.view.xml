<mvc:View 
	controllerName="de.integrata.training.opentext.controller.Master"
	xmlns="sap.m"
	xmlns:core="sap.ui.core" 
	xmlns:mvc="sap.ui.core.mvc">
	<Page title="{i18n>MasterPageTitle}">
		<content>
			<List items="{/FlugkundeSet}">
				<items>
					<StandardListItem title="{Custname}" description="{City}" type="Active" press="onItemPress"/>
				</items>
			</List>
		</content>
	</Page>
</mvc:View>